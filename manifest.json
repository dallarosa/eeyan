{
  "name": "ええやん！",
    "version": "0.1",
    "manifest_version": 2,
    "description": "フェイブック上の全ての「いいね！」を「ええやん！」にする拡張機能です。",
    "default_icon": "eeyan.png",
    "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["jquery.js", "myscript.js"]
    }
  ],
    "permissions": [
      "tabs",
      "https://www.facebook.com/"
      ]
}
